{
    "name": "bdp-raymon/otp-limiter",
    "description": "Otp Limiter",
    "keywords": [
        "raymon",
        "otp-limiter"
    ],
    "homepage": "https://github.com/raymon/otp-limiter",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ali Shabani",
            "email": "alishabani9270@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.7",
        "phpunit/phpunit": "^9.3",
        "psalm/plugin-laravel": "^1.4",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "BdpRaymon\\OtpLimiter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BdpRaymon\\OtpLimiter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "BdpRaymon\\OtpLimiter\\OtpLimiterServiceProvider"
            ]
        }
    }
}
